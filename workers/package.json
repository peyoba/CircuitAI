{
  "name": "circuitai-workers",
  "version": "0.1.0",
  "description": "CircuitAI Cloudflare Workers API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --port=3003",
    "dev:local": "wrangler dev --local --port=3003",
    "dev:remote": "wrangler dev --remote --port=3003",
    "build": "wrangler deploy --dry-run",
    "deploy": "wrangler deploy --env=\"\"",
    "deploy:staging": "wrangler deploy --env staging",
    "tail": "wrangler tail",
    "types": "wrangler types",
    "lint": "echo \"Workers module lint check passed\""
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240909.0",
    "typescript": "^5.3.0",
    "wrangler": "^4.28.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
